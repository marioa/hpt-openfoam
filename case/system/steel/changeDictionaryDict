T
{
    internalField   uniform $Tref;

    boundaryField
    {
       ".*"
        {
            type            zeroGradient;
            value           uniform $Tref;
        }
        "steel_to_.*"
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;
            Tnbr            T;
            kappaMethod     solidThermo;
            value           uniform $Tref;
        }
       up
            {
                type            fixedGradient; // change it to heat transfer with the room!!
                gradient        uniform 4;//$gradroom;
            }
       ref
            {
                type            fixedValue;
                value           uniform $Tref;
            }
       "wedge.*"
            {
                type            wedge;
          	    physicalType	  wedge;
            }
       steelwalls
            {
                type            zeroGradient;//wall;
	              //physicalType  	wall;
            }
    }
}

