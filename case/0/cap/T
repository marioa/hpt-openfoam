FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/cap";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 300;

boundaryField
{
    wedge0
    {
        type            wedge;
        physicalType    wedge;
    }
    wedge1
    {
        type            wedge;
        physicalType    wedge;
    }
    cap_to_food
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           uniform 300;
        Tnbr            T;
        kappaMethod     solidThermo;
        kappaName       none;
        thicknessLayers ( 0.001 );
        kappaLayers     ( 0.0005 );
    }
    cap_to_fluid
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           uniform 300;
        Tnbr            T;
        kappaMethod     solidThermo;
        kappaName       none;
        thicknessLayers ( 0.001 );
        kappaLayers     ( 0.0005 );
    }
}


