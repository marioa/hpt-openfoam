FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/steel";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 0;

boundaryField
{
    wedge0
    {
        type            wedge;
    }
    steelwalls
    {
        type            zeroGradient;
    }
    up
    {
        type            zeroGradient;
    }
    wedge1
    {
        type            wedge;
    }
    ref
    {
        type            zeroGradient;
    }
    steel_to_fluid
    {
        type            calculated;
        value           uniform 0;
    }
    steel_to_food
    {
        type            calculated;
        value           uniform 0;
    }
}


