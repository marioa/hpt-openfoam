FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/steel";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 293;

boundaryField
{
    wedge0
    {
        type            wedge;
        value           uniform 293;
        physicalType    wedge;
    }
    steelwalls
    {
        type            zeroGradient;
        value           uniform 300;
    }
    up
    {
        type            externalWallHeatFluxTemperature;
        value           uniform 293;
        kappaMethod     solidThermo;
        Ta              uniform 292.45;
        h               uniform 28;
        thicknessLayers ( 0 );
        kappaLayers     ( 0 );
        kappaName       kappa;
        Qr              none;
        relaxation      0.15;
        mode            coefficient;
    }
    wedge1
    {
        type            wedge;
        value           uniform 293;
        physicalType    wedge;
    }
    ref
    {
        type            fixedValue;
        value           uniform 293;
    }
    steel_to_fluid
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           uniform 293;
        Tnbr            T;
        kappaMethod     solidThermo;
    }
    steel_to_food
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           uniform 293;
        Tnbr            T;
        kappaMethod     solidThermo;
    }
}


