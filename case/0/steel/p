FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/steel";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 101325;

boundaryField
{
    wedge0
    {
        type            wedge;
        physicalType    wedge;
    }
    steelwalls
    {
        type            calculated;
        value           uniform 101325;
    }
    up
    {
        type            calculated;
        value           uniform 101325;
    }
    wedge1
    {
        type            wedge;
        physicalType    wedge;
    }
    ref
    {
        type            calculated;
        value           uniform 101325;
    }
    steel_to_fluid
    {
        type            calculated;
        value           uniform 0;
    }
    steel_to_food
    {
        type            calculated;
        value           uniform 0;
    }
}


